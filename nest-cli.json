{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "projects": {
    "helloWorld": {
      "type": "application",
      "root": "src/helloWorld",
      "entryFile": "main",
      "sourceRoot": "src/helloWorld",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.json",
        "deleteOutDir": true
      }
    }
  }
}
